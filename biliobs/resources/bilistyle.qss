    border: 1px solid #EEEEEE;
    border-radius: 4px;
    padding: 1px 2px 1px 2px;

	font-family: "Microsoft YaHei";
    font-size: 12px;
    color: #798CA7;
}
QFontComboBox:on { /* shift the text when the popup opens */
    padding-top: 3px;
    padding-left: 4px;
}
QFontComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;

    border-left-width: 0px;
    border-left-color: #EEEEEE;
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 4px; /* same radius as the QComboBox */
    border-bottom-right-radius: 4px;
}
QFontComboBox::down-arrow {
	image: url(:/FucBtn/ComBOpen);
}
QFontComboBox::down-arrow:on { /* shift the arrow when popup is open */
	image: url(:/FucBtn/ComBClose);
    top: 1px;
    left: 1px;
}
QFontComboBox QAbstractItemView {

    border: 2px solid #EEEEEE;
    selection-background-color: #8FC5E3;
	border-color:#DDDDDD;

	font-family: "Microsoft YaHei";
    font-size: 12px;
    color: #798CA7;

	border-radius: 4px;

}
QFontComboBox QScrollBar:vertical {
	border: 0px;
	background: #FFFFFF;
	width: 8px;
	margin: 10px 0 10px 0;
}
QFontComboBox QScrollBar::handle:vertical {
	background: #DDDDDD;
	min-height: 45px;
	border-radius: 4px;
	/*
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	*/
}
QFontComboBox QScrollBar::add-line:vertical {
	border: 0px;
	background: #FFFFFF;
	height: 0px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}
QFontComboBox QScrollBar::sub-line:vertical {
	border: 0px;
	background: #FFFFFF;
	height: 0px;
	subcontrol-position: top;
	subcontrol-origin: margin;
}
QFontComboBox QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
	border: 0px;
	width: 8px;
	height: 0px;
	background: #FFFFFF;
}
QFontComboBox QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: #FFFFFF;
