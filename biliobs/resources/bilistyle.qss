QSlider::groove:horizontal {
	border: 1px solid #DDDDDD;
	height: 2px;
	background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #DDDDDD, stop:1 #DDDDDD);
}
QSlider::sub-page:horizontal {
	background: #4FC1E9;
}
QSlider::handle:horizontal {
	background: qlineargradient(x1:0, y1:0, x2:0.3, y2:0.3, stop:0 #FFFFFF, stop:1 #DDDDDD);
	border: 1px solid #DDDDDD;
	width: 14px;
	height: 14px;
	margin: -7px ;
	border-radius: 7px;
}
